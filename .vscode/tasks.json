    "version": "2.0.0",
    "tasks": [
        {
            "label": "kernel-riscv64",
            "type": "shell",
            "isBackground": true,
            "command": "cargo make debug-riscv64",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                {
                    "pattern": {
                    "regexp": "Starting",
                    "line": 1,
                    },
                    "background": {
                        "activeOnStart": true,
                        "beginsPattern": "Starting",
                        "endsPattern": "Starting"
                    }
                }
            ],
